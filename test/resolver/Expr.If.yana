fn unit(a: Int) = a

fn a(a: Int):
    if a > 0:
        return unit(a)
    else:
        return unit(a - 1)

fn b(a: Int, b: Int) = if a > b then a else b

fn c(a: Int, b: Int):
    let &result = 0
    if:
        a == 0 -> result = 1
        b == 0 -> result = 2
        else   -> result = 3
    return result * 2
